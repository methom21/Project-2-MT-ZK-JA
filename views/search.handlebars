<div><h2>SEARCH TEST</h2></div>
  <div id= 'search-form'>
    <form class="form-group">
    <input
      id="searchInput"
      type="text"
      class="form-control"
      placeholder="Enter Hero Name"
      aria-label="Enter Hero Name"
      aria-describedby="button-addon2"
    />
    </form>
    <button class="btn btn-primary search-button" type="button" id="searchButton" >Search<span>Search</span></button>
  </div>

  <div>
    <div id="searchResults">
    </div>
  </div>


  <script id="cardTemplate" type="text/x-handlebars-template">
    {{#each response}}
      <div>
      <h2>{{name}}</h2>

      </div>
    {{/each}}
  </script>
</div>


  <script src="/js/search.js"></script>